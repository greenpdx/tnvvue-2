    "rules": {
      "no-undef": "warn",
      "no-console": "off",
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
